  ColorSequenceKeypoint.new(1, Color3.fromRGB(118, 50, 255)),
})
uIGradient1.Parent = magichappens
uIGradient1.Offset = Vector2.new(-0.3, 0)

local tween = TweenService:Create(
	uIGradient1,
	TweenInfo.new(
		2.5, 
		Enum.EasingStyle.Sine,
		Enum.EasingDirection.InOut,
		-1, 
		true 
	),
	{ Offset = Vector2.new(0.3, 0) }
)

task.delay(0.05, function()
	tween:Play()
end)


